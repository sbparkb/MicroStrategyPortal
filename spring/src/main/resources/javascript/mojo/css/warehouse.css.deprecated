    overflow: visible;
    height: auto !important;
    width: auto !important;
    padding: 6px;
}
.item-mn-proxy-pnlicns {
    background: url('images/wh/panel_icons.png') no-repeat 0px -90px;
    overflow: visible;
    height: auto !important;
    width: auto !important;
    padding: 6px;
}
/** Menu End **/