/** Menu Start **/
.mstrmojo-ui-Menu {
    margin-left: -1px;
}

.mstrmojo-ui-Menu .item {
    margin: 1px;
    border: 0;
    padding-left: 25px;
}
.mstrmojo-ui-Menu a.item.sep.disabled {
    border-top: 1px dotted #CCC;
    padding: 0;
    margin: 0;
    opacity: 1;
}

.mstrmojo-ui-Menu a.item.sep:first-child,
.mstrmojo-ui-Menu a.item.sep:last-child{
    border: 0;
}

.mstrmojo-ui-Menu .item > DIV.arw {
    background: url('images/wh/arrow-right.png') no-repeat center center;
    top: 0;
    right: -2px;
    height: 25px;
}

.mstrmojo-ui-Menu .item.on {
    background: none;
    color: #656565;
}

.mstrmojo-ui-Menu .item.on .micn {
    background: url('images/wh/ok.png') no-repeat center center;
}

.mstrmojo-ui-Menu .item > DIV.micn {
    left: 5px;
}

.mstrmojo-ui-MenuEditor {
    background-color: #fff;
    margin-left: -1px;
}

.mstrmojo-ui-MenuEditor-buttons {
    height: 32px;
    border-top: 1px dotted #BBB;
    padding-top: 1px;
}

.mstrmojo-ui-MenuEditor-buttons .mstrmojo-Button {
    background-color: #f5f5f5;
    border: 1px solid #AAA;
