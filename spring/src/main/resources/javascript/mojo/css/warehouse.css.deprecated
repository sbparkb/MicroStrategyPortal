input {
    padding: 0;
    margin: 0;
    outline: none;
    color: #666;
}

.mstrmojo-SplitterHandle{
    box-sizing: border-box;
    -moz-box-sizing: border-box;
}

.mstrmojo-Editor-button.rightfloat {
    float: right;
}

/** List and Tree Item Common CSS Start **/
.item-n {
    white-space: nowrap;
    text-overflow: ellipsis;
    -ms-user-select: none;
}

.item-mn {
    background: transparent url('images/wh/menu.png') no-repeat center center;
    padding: 6px;
    margin: 3px;
    position: absolute;
    right: -20px;
    top: 0;
    -webkit-transition: opacity 300ms;
    opacity: 0;
    visibility: visible;
}

.item-mn:hover {
    cursor: pointer;
}

.item:hover .item-mn {
    right: 4px;
    opacity: 1;
}

.item-ic {
    background: transparent url(images/wh/object_icons.png) no-repeat center center;
    padding: 4px 9px;
    overflow: hidden;
}

.item.tp12 .item-ic {
    background-position: 0 -324px;
}

.item.tp13 .item-ic {
    background-position: 0 -299px;
}

.item.tp21 .item-ic {
    background-position: 0 -351px;
}

.item.tp26 .item-ic {
    background-position: 1px -122px;
}

.item.tp29 .item-ic {
    background-position: 3px -21px;
}

.item.tp29 .item-ic.shared {
    background-position: 3px -47px;
}

.item.tp1001 .item-ic {
    background-position: 0 -350px;
}

.item.tpnone .item-ic {
    background: none;
    padding: 4px 0 4px 1px;
}
.item.tpnone .item-mn {
    display: none;
}
/** List and Tree Item Common CSS End **/

/** TablesTree Start **/
.mstrmojo-wh-Panel.ProjectTables {
    background-color: #f6f6f6;
}

.mstrmojo-wh-TablesTree {
    overflow: auto;
    background-color: #f6f6f6;
}

.mstrmojo-wh-TablesTree li {
    transition: background-color 300ms;
}

.mstrmojo-wh-TablesTree li .item {
    background: transparent url(images/wh/object_icons.png) no-repeat left center;
    padding-left: 18px;
    position: relative;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.mstrmojo-wh-TablesTree .item-mn {
    top: 1px;
}

.mstrmojo-wh-TablesTree li.selected > .item,
.mstrmojo-wh-TablesTree li .item.bld {
    font-weight: bold;
    color: #252525;
}

.mstrmojo-wh-TablesTree .item.tp15 {
    background-position: left -97px;
}

.mstrmojo-wh-TablesTree .item.tp26 {
    background-position: left -122px;
}

.mstrmojo-wh-TablesTree .item.tp26.uc {
    background-position: left -147px;
}

.mstrmojo-wh-TablesTree li.userSel {
    background-color: #e1e1e1;
}

.mstrmojo-wh-TablesTree li.userSel > ul {
    background-color: #f6f6f6;
}

.mstrmojo-wh-TablesTree.mltSel li.userSel .item-mn {
    background-color: #FFF8EC;
    border-color: #FFA500;
}

.mstrmojo-wh-TablesTree.hasOwnAvatar {
    position: absolute;
    border: 1px solid #B6B6B6;
    background: #f3f3f3;
    margin: 2px;
}

.mstrmojo-wh-TablesTree.hasOwnAvatar > div:last-child {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    width: 22px;
    background: #F3F3F3 url(images/wh/ban.png) no-repeat center center;
}

.mstrmojo-wh-TablesTree.hasOwnAvatar.enabled > div:last-child {
    display: none;
}

.mstrmojo-wh-TablesTree.hasOwnAvatar li {
    background: none;
    padding: 0 5px;
}

.mstrmojo-wh-TablesTree.hasOwnAvatar li .item-mn,
.mstrmojo-wh-TablesTree.hasOwnAvatar li ul {
    display: none;
}

/** TablesTree End **/

/* Base Panel start */
.mstrmojo-Splitter.WHPanel {
    padding-top: 2px;
}

.mstrmojo-wh-Panel {
    border: 1px solid #aaa;
    overflow: hidden;
    background-color: #E5E5E5;
    position: relative;
    box-sizing: border-box;
    -moz-box-sizing: border-box;
}

.mstrmojo-wh-pn-header {
    background-color: #f6f6f6;
    border-bottom: 1px solid #ddd;
    box-shadow: 0 1px 0 #fff;
    white-space: nowrap;
    line-height: 35px;
    height: 35px;
    position: relative;
}

.mstrmojo-wh-pn-title {
    height: 35px;
    line-height: 35px;
    font-weight: bold;
    padding-left: 25px;
    float: left;
    text-transform:uppercase;
    background: transparent url(images/wh/object_icons.png) no-repeat 7px -66px;
    display: block;
}

.mstrmojo-wh-pn-instructions-text{
    text-align: center;
    width: 100%;
    display: none;
    font-size: 16pt;
    color: darkgray;
    font-style: italic;
}

.mstrmojo-wh-pn-title.hidden {
    display: none;
}

.mstrmojo-wh-pn-ttl-icons {
    float: right;
    cursor: pointer;
    position: absolute;
    right: 0;
    display: block;
}

.mstrmojo-wh-pn-ttl-icons.hidden {
    display: none;
}

.mstrmojo-wh-pn-ttl-icons span {
    height: 30px;
    width: 30px;
    padding: 0;
    margin: 2px;
    display: inline-block;
    opacity: 0.8;
    transition: opacity 200ms;
    background: url('images/wh/panel_icons.png') no-repeat center center;
}

.mstrmojo-wh-pn-ttl-icons span:hover {
    background-color: #eee !important;
    opacity: 1;
}

/* Base Panel end */

/* DB Panel start */

.mstrmojo-wh-Panel.DBConnections {
    background-color: #f6f6f6;
}

.mstrmojo-wh-Panel.DBConnections .mstrmojo-wh-pn-title {
    background-position: 7px 11px;
}

.mstrmojo-wh-dbConnectionslist {
    margin: 0 2px 0 0;
    overflow: auto;
    padding-left: 1px;
}

.mstrmojo-wh-dbConnectionslist .item {
    padding: 4px;
    border-bottom: 1px solid transparent;
    -webkit-transition: all 300ms;
    overflow: hidden;
    position: relative;
    cursor: pointer;
    text-overflow: ellipsis;
}

.mstrmojo-wh-dbConnectionslist  .item.selected {
    background-color: #e1e1e1;
}
.mstrmojo-wh-dbConnectionslist .item-n {
    line-height: 14px;
}
.mstrmojo-wh-dbConnectionslist .item.primary .item-n{
    font-weight: bold;
}
/* DB Panel end*/

/** Available Tables Tree start */
.mstrmojo-wh-AvailableTablesTree li[level="0"] > ul {
    padding-left: 10px;
}

.mstrmojo-wh-AvailableTablesTree li[level="1"] {
    background: none !important;
}

.mstrmojo-wh-AvailableTablesTree .item.tp26 .item-mn {
    display: none;
}

/** Available Tables Tree end */
.mstrmojo-wh-Panel.AvailableTables {
    background-color: #f6f6f6;
}

.mstrmojo-wh-Panel.AvailableTables .mstrmojo-PopupList {
    z-index: 1;
}

.mstrmojo-wh-Panel.AvailableTables .mstrmojo-ui-Pulldown-text {
    height: 10px;
    line-height: 10px;
    max-width: 100%;
    margin-top: 2px;
    border: 1px solid #AAA;
    outline: none;
}

.mstrmojo-wh-Panel.AvailableTables .mstrmojo-ui-Pulldown.locked div.mstrmojo-ui-Pulldown-text{
    background: none;
}

/** NameSpaces Pulldown start **/

.mstrmojo-wh-Panel.AvailableTables .mstrmojo-ui-Pulldown .mstrmojo-PopupList .item {
    max-width: 100%;
}

.mstrmojo-wh-Panel.AvailableTables .mstrmojo-ui-Pulldown {
    margin-right: 2px;
    margin-left: 2px;
}

/** NameSpaces Pulldown end **/

/* SearchBox start*/
.mstrmojo-wh-SearchBox {
    padding: 2px;
    margin-bottom: 2px;
    border-bottom: 1px solid #ddd;
    background-color: #fff;
}

.mstrmojo-wh-SearchBox div {
    float: left;
    overflow: hidden;
}

.mstrmojo-wh-sb-input {
    float: left;
    margin: 0 -22px 0 0;
    padding: 0 0 0 20px;
    height: 20px;
    line-height: 16px;
    border: 1px solid #ddd;
    border-radius: 3px;
    border-right: 0;
    outline: none;
    background: transparent url(images/wh/panel_icons.png) no-repeat 5px -208px;
}

.mstrmojo-wh-sb-btn {
    height: 20px;
    width: 22px !important;
    border: 1px solid #ddd;
    border-left-width: 0;
    border-radius: 3px;
    cursor: pointer;
}

.mstrmojo-wh-sb-btn.clear {
    background: transparent url(images/wh/utils.png) no-repeat -20px 7px;
}
.mstrmojo-wh-sb-btn.clear:hover {
    background-position: -20px -23px;
}

/* SearchBox end */

/** Icons Start **/
.mstrmojo-wh-info {
    background: #EFEFEF url('images/wh/info.png') no-repeat center center;
    border: 0 !important;
}

.mstrmojo-wh-pn-ttl-icons span.mstrmojo-wh-refresh {
    background-position: 8px 7px;
}

.mstrmojo-wh-pn-ttl-icons span.mstrmojo-wh-refresh.hidden {
    visibility: hidden;
}

.mstrmojo-wh-pn-ttl-icons span.mstrmojo-wh-addDB {
    background-position: 8px -53px;
}

.mstrmojo-wh-pn-ttl-icons span.mstrmojo-wh-catalogSQL {
    background-position: 8px -562px;
}

.mstrmojo-wh-pn-ttl-icons span.mstrmojo-wh-catalogSQL.hidden {
    visibility: hidden;
}
/** Icons End **/

/** Catalog SQL Editor Start**/

