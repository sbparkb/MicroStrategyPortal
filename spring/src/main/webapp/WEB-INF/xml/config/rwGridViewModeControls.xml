<shortcut-list name="gridViewModes">
     <attributes>
       <attribute name="ty" value="btng"/>
       <attribute name="nowrap" value="1"/>
     </attributes>
     <shortcut desc="[View: Grid]" desc-id="mstrWeb.3547" feature-id="menu-grid-mode" name="tbGrid" type="button">
