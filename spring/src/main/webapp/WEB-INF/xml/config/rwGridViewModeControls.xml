         <attribute name="cmdvl" value="3"/>
       </attributes>
     </shortcut>
     <shortcut desc="[View: Flash]" desc-id="mstrWeb.14513" feature-id="visualizations-supported;visualizations-available-for-rw-grids" name="tbFlash" type="button" iframe="false">
