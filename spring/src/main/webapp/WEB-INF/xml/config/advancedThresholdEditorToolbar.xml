        </shortcut>
        <shortcut desc="[Highest % Value]" desc-id="mstrWeb.4588" name="pkrAte2">
          <attributes>
            <attribute name="class" value="ateHighestPValue" />
          </attributes>
        </shortcut>
        <shortcut desc="[Lowest % Value]" desc-id="mstrWeb.4589" name="pkrAte1">
          <attributes>
            <attribute name="class" value="ateLowestPValue" />
          </attributes>
        </shortcut>
      </shortcut-list>
    </shortcut>
    <shortcut desc="[New Threshold]" desc-id="mstrWeb.4585" name="tbNewThresholdRW" type="button" feature-id="is-conditional-formatting">
      <attributes>
        <attribute name="cmdtp" value="0" />
        <attribute name="cmdid" value="ateNewThreshold" />
      </attributes>
    </shortcut>
