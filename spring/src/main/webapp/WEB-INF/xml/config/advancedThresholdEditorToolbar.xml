      <attribute name="ty" value="btng" />
      <attribute name="nowrap" value="1"/>
    </attributes>
    <shortcut desc="[New]" desc-id="mstrWeb.4996" name="tbNewThreshold" type="textcombo" feature-id="!is-conditional-formatting" textCombo-tooltip-desc-id="mstrWeb.6168">
      <attributes>
        <attribute name="cmdid" value="ateNewThreshold" />
        <attribute name="cmdtp" value="4" />
        <attribute name="cmdpk" value="newThresholdPicker" />
      </attributes>
      <shortcut-list name="newThresholdPicker">
        <attributes>
          <attribute name="ty" value="pkr" />
        </attributes>
        <shortcut desc="[Highest Value]" desc-id="mstrWeb.4586" name="pkrAte5">
          <attributes>
            <attribute name="class" value="ateHighestValue" />
          </attributes>
        </shortcut>
        <shortcut desc="[Lowest Value]" desc-id="mstrWeb.4587" name="pkrAte4">
          <attributes>
            <attribute name="class" value="ateLowestValue" />
