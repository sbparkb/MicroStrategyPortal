<!DOCTYPE mstrlayout:layout SYSTEM "mstrlayout.dtd">
<mstrlayout:layout>
    <mstrlayout:if method="isDhtml">
        <mstrlayout:then>
		  <mstrlayout:render name="tabContainerBody"/>
		  <mstrlayout:render name="controlBar"/>
		</mstrlayout:then>
		<mstrlayout:else>
		  <mstrlayout:render name="controlBar"/>	
		  <mstrlayout:render name="tabContainerBody"/>
		</mstrlayout:else>
    </mstrlayout:if>
</mstrlayout:layout>
