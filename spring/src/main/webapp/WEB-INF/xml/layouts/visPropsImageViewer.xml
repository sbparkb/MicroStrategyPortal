<!DOCTYPE mstrlayout:layout SYSTEM "mstrlayout.dtd">
<mstrlayout:layout>
      <![CDATA[
        <style type="text/css">
            .t {
                width:100%;
                padding:2px 2px 2px 10px;
            }   
            .b {
                text-align:left;
                padding-left:20px; 
                    
            }
            .leftpadding {
            	padding-left: 9px;
            	width: 129px; 
            }
            .middlepadding {
            	padding-left: 29px;
            	width: 129px; 
            }
            .g{
            	max-width:180px; 
            	display: inline-block; 
            }
        </style>
     ]]>

	<table class="t">
       <tr>
          <td class="leftpadding">
            <mstrlayout:render name="descriptor">
              <mstrlayout:argument type="string" value="mstrWeb.5258"/> <!-- Descriptor: Display style: -->
            </mstrlayout:render>
          </td>
          <td class="b"> 
	          <div class="g">
	            <mstrlayout:render name="SelectBox">
					<mstrlayout:argument type="String" value="ds" /> <!-- prop name -->
					<mstrlayout:argument type="String" value="0^mstrWeb.9052|1^mstrWeb.9053|2^mstrWeb.9054" /> <!-- selectBox elements -->
					<mstrlayout:argument type="String" value="onchange^microstrategy.bone('visPropsEditor_visPropsImageViewerStyle').updateDisplayStylePulldown();" /> <!-- extra att -->
					<mstrlayout:argument type="String" value="0" /> <!-- default val -->
				</mstrlayout:render>
	          </div>  
          </td>
        </tr>
        <tr id="sdSelect">
          <td class="middlepadding">
            <mstrlayout:render name="descriptor">
              <mstrlayout:argument type="string" value="mstrWeb.9055"/> <!-- Descriptor: Scroll Direction: -->
            </mstrlayout:render> 
          </td>
          <td class="b"> 
	          <div class="g">
	            <mstrlayout:render name="SelectBox">
					<mstrlayout:argument type="String" value="sd" /> <!-- prop name -->
					<mstrlayout:argument type="String" value="0^mstrWeb.2069|1^mstrWeb.2066" /> <!-- selectBox elements -->
				</mstrlayout:render>
	          </div>  
          </td>
        </tr>
        <tr id="norSelect">
           <td class="middlepadding">
            <mstrlayout:render name="descriptor">
              <mstrlayout:argument type="string" value="mstrWeb.9059"/> <!-- Descriptor: # of Rows: -->
            </mstrlayout:render>
          </td>
          <td class="b"> 
	          <div class="g">
	            <mstrlayout:render name="Textbox">
					<mstrlayout:argument type="String" value="nor" /> <!-- prop name -->
					<mstrlayout:argument type="String" value="isValid^microstrategy.bone('visPropsEditor_visPropsImageViewerStyle').validateDataType();" /> <!-- prop name -->
					<mstrlayout:argument type="String" value="3" /> <!-- prop name -->
				</mstrlayout:render>
	          </div>  
          </td>
        </tr>
        <tr id="nocSelect">
           <td class="middlepadding">
            <mstrlayout:render name="descriptor">
              <mstrlayout:argument type="string" value="mstrWeb.9060"/> <!-- Descriptor: # of Columns: -->
            </mstrlayout:render>
          </td>
          <td class="b"> 
	          <div class="g">
	            <mstrlayout:render name="Textbox">
					<mstrlayout:argument type="String" value="noc" /> <!-- prop name -->
					<mstrlayout:argument type="String" value="isValid^microstrategy.bone('visPropsEditor_visPropsImageViewerStyle').validateDataType();" /> <!-- prop name -->
					<mstrlayout:argument type="String" value="2" /> <!-- prop name -->
				</mstrlayout:render>
	          </div>  
          </td>
        </tr>
        <tr>
          <td class="leftpadding">
            <mstrlayout:render name="descriptor">
              <mstrlayout:argument type="string" value="mstrWeb.9056"/> <!-- Descriptor: First caption line: -->
            </mstrlayout:render>
          </td>
          <td class="b"> 
	          <div class="g">
	            <mstrlayout:render name="AttributeFormsPulldown">
					<mstrlayout:argument type="String" value="fcl" /> <!-- prop name -->
				</mstrlayout:render>
	          </div>  
          </td>
        </tr>
        <tr>
          <td class="leftpadding">
            <mstrlayout:render name="descriptor">
              <mstrlayout:argument type="string" value="mstrWeb.9057"/> <!-- Descriptor: Second caption line:  -->
            </mstrlayout:render>
          </td>
          <td class="b"> 
	          <div class="g">
	            <mstrlayout:render name="AttributeFormsPulldown">
					<mstrlayout:argument type="String" value="scl" /> <!-- prop name -->
				</mstrlayout:render>
	          </div>  
          </td>
        </tr>
        <tr id="dActForm">
            <td class="leftpadding">
            <mstrlayout:render name="descriptor">
              <mstrlayout:argument type="string" value="mstrWeb.8306"/> <!-- Descriptor: Default Action Form -->
            </mstrlayout:render>
          </td>
          <td class="b"> 
	          <div class="g">
	            <mstrlayout:render name="AttributeFormsPulldown">
					<mstrlayout:argument type="String" value="daf" /> <!-- prop name -->
				</mstrlayout:render>
	          </div>  
          </td>
        </tr>
        <tr>
         <td class="leftpadding">
            <mstrlayout:render name="descriptor">
              <mstrlayout:argument type="string" value="mstrWeb.8015"/> <!-- Descriptor: Background Color: -->
            </mstrlayout:render>
          </td>
          <td class="b"> 
	          <div class="g">
	            <mstrlayout:render name="ColorPicker">
					<mstrlayout:argument type="String" value="bgc" /> <!-- prop name -->
					<mstrlayout:argument type="String" value="#CCCCCC" />
				</mstrlayout:render>
	          </div>  
          </td>
        </tr>
        <tr>
          <td class="leftpadding">
           <mstrlayout:render name="descriptor">
              <mstrlayout:argument type="string" value="mstrWeb.7819"/> <!-- Descriptor: Border Color: -->
            </mstrlayout:render>
          </td>
          <td class="b"> 
	          <div class="g">
	            <mstrlayout:render name="ColorPicker">
					<mstrlayout:argument type="String" value="bc" /> <!-- prop name -->
					<mstrlayout:argument type="String" value="#FFFFFF" />
				</mstrlayout:render>
	          </div>  
          </td>
        </tr>
        <tr>
          <td class="leftpadding">
            <mstrlayout:render name="descriptor">
              <mstrlayout:argument type="string" value="mstrWeb.9058"/> <!-- Descriptor: Border Width:  -->
            </mstrlayout:render>
          </td>
          <td class="b"> 
	          <div class="g">
	            <mstrlayout:render name="BorderWidthSelectBox">
					<mstrlayout:argument type="String" value="bw" /> <!-- prop name -->
				</mstrlayout:render>
	          </div>  
          </td>
        </tr>
       </table>      
</mstrlayout:layout>