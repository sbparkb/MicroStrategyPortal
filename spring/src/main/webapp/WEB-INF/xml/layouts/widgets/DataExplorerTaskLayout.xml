<!DOCTYPE mstrlayout:layout SYSTEM "mstrlayout.dtd">
<mstrlayout:layout>
	<mstrlayout:if method="isElementList">
		<mstrlayout:then>
			<mstrlayout:render name="self">
				<mstrlayout:argument value="WidgetElementListStyle" />
			</mstrlayout:render>
		</mstrlayout:then>
		<mstrlayout:else>
			<mstrlayout:render name="self">
				<mstrlayout:argument value="WidgetDataExplorerFolderStyle" />
			</mstrlayout:render>
		</mstrlayout:else>
	</mstrlayout:if>
</mstrlayout:layout>