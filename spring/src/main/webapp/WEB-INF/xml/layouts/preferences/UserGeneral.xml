              <hr style="height: 1px;"/>
            </td>
          </tr>

          <tr>
            <td valign="top" style="width:18%;">
              <span class="prefs-input" tabindex="0">
                <mstrlayout:render name="descriptor"> <!-- Color theme: -->
                  <mstrlayout:argument type="String" value="mstrWeb.5584"/>
                </mstrlayout:render>
              </span>
            </td>
            <td>
              <div class="mstrColorThemeList">
