                        </mstrlayout:render>
                      </div>
                    </td>
                  </tr>
                  <tr>
                    <td colspan="2">
                      <mstrlayout:render name="AsCheckBox">
                        <mstrlayout:argument type="string" value="com.microstrategy.web.objects.EnumWebPreferences.WebPreferenceCopyNameTranslations" />
