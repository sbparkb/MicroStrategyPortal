                    <mstrlayout:argument type="string" value="com.microstrategy.web.objects.EnumWebPreferences.WebPreferenceShowFolderUpBtnInNavBar" />
                    <mstrlayout:argument type="string" value="mstrWeb.14384" /> <!--Descriptor:-->
                  </mstrlayout:render>
                </td>
               </tr>
              </table>
