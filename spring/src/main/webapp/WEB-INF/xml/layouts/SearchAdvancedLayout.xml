<!DOCTYPE mstrlayout:layout SYSTEM "mstrlayout.dtd">
<mstrlayout:layout>
  <table cellspacing="0" cellpadding="0" border="0">
    <colgroup>
      <col width="*"/>
      <col width="20"/>
      <col width="*"/>
    </colgroup>
    <tr>
      <td style="vertical-align: top">
        <div class="mstrSearchSections">
          <div id="nameSection">
            <mstrlayout:render name="searchNameTitle"/>
            <br />
            <mstrlayout:render name="searchNameTextInputBox"/>
            <br />
            <mstrlayout:render name="searchStringConstraintsComboBox"/>
            <mstrlayout:render name="requiredLabelForSearchName"/>
            <br />
          </div>
          <hr />
          <div id="locationSection">
            <mstrlayout:render name="locationTitle"/>
            <mstrlayout:render name="searchLocationAllFolders"/>
            <br />
            <mstrlayout:if name="hasCurrentSearchFolder">
              <mstrlayout:then>
                <mstrlayout:render name="searchLocationCurrentFolder"/>
                <br />
              </mstrlayout:then>
            </mstrlayout:if>
            <mstrlayout:render name="searchLocationSharedReportsFolder"/>
            <br />
            <mstrlayout:render name="searchLocationMyReportFolder"/>
          </div>
          <hr />
          <div id="typesSection">
            <mstrlayout:render name="typeTitle"/>
            <br />
            <mstrlayout:render name="searchObjectTypeReports"/>
            <br />
            <mstrlayout:render name="searchObjectTypeDocuments"/>
            <br />
            <mstrlayout:render name="searchObjectTypeHTMLDocuments"/>
            <br />
            <mstrlayout:render name="searchObjectTypeFolders"/>
            <br />
            <mstrlayout:render name="searchObjectTypeFilters"/>
            <br />
            <mstrlayout:render name="searchObjectTypeCustomGroups"/>
            <br />
            <mstrlayout:render name="searchObjectTypeTemplates"/>
            <br />
            <mstrlayout:render name="searchObjectTypePrompts"/>
            <br />
            <mstrlayout:render name="searchObjectTypeShortcuts"/>
            <br />
          </div>
          <hr />
          <div id="datesSection">
            <mstrlayout:render name="dateTitle"/>
            <br />
            <mstrlayout:render name="dateSimpleFindAllObject"/>
            <br />
            <mstrlayout:render name="findAllObjectsWithCreatedOrModified"/>
            <br />
            <div id="dateAdvanced">
              <mstrlayout:render name="dateForPreviousTimeUnits"/>
              <br />
              <mstrlayout:render name="dateStartAndEndDate"/>
            </div>
          </div>
          <hr />
          <div id="ownerSection">
            <mstrlayout:render name="ownerTitle"/>
            <br />
            <mstrlayout:render name="ownerCreatedByAnyUser"/>
            <br />
            <mstrlayout:render name="ownerCreatedOnlyByMe"/>
          </div>
          <hr />
          <div id="descriptionSection">
            <mstrlayout:render name="descriptionTitle"/>
            <br />
            <mstrlayout:render name="descriptionTextArea"/>
          </div>
          <hr />
          <div id="searchOptions">
            <mstrlayout:render name="searchOptionsTitle"/>
            <br />
            <mstrlayout:render name="searchOptionsDerefrenceShortcut"/>
          </div>
        </div>
      </td>
      <td>&nbsp;</td>
      <td style="vertical-align: top">
        <div style="vertical-align: top">
          <mstrlayout:render name="searchDetails"/>
          <mstrlayout:render name="newSearchLink"/>
          <mstrlayout:render name="searchTypeLink"/>
        </div>
      </td>
    </tr>
  </table>
</mstrlayout:layout>
