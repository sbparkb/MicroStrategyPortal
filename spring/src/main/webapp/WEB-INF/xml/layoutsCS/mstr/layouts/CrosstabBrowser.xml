<mstrlayout:layout xmlns:mstrlayout="http://microstrategy.com/web/2008/CSLAYOUT"
    on_set_visible="mstr.behaviors.Generic.set_css_display(rootTag, 'block', 'none', view.props['visible'])">
  <div style="{@cssText}"
       class="{@cssPrefix}{@cssClass}"
       id="{@id}">
    <table cellspacing="0"
           cellpadding="0"
           class="{@cssPrefix}{@cssClass}FetchTable">
      <tr>
        <td class="{@cssPrefix}{@cssClass}RowFetchCell">
          <mstrlayout:wRender name="rowFetchView">
          </mstrlayout:wRender>
        </td>
        <td class="{@cssPrefix}{@cssClass}ColumnFetchCell">
          <mstrlayout:wRender name="columnFetchView">
          </mstrlayout:wRender>
        </td>
      </tr>
    </table>
    <div class="{@cssPrefix}{@cssClass}ContainerContentsView">
      <mstrlayout:wRender name="preview">
      </mstrlayout:wRender>
      <mstrlayout:wRender name="contentsView">
        Loading...
      </mstrlayout:wRender>
    </div>
  </div>
</mstrlayout:layout>
