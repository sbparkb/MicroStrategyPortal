<mstrlayout:layout xmlns:mstrlayout="http://microstrategy.com/web/2008/CSLAYOUT"
   
    on_set_visible="mstr.behaviors.Generic.set_css_display(rootTag, 'block', 'none', view.props['visible'])">
  <div title="{@tooltip}"
       class="{@cssPrefix}{@cssClass}"
       id="{@id}">
    <table cellspacing="0"
           cellpadding="0"
           class="{@cssPrefix}{@cssClass}Table">
      <tr>
        <td class="{@cssPrefix}{@cssClass}SearchBox">
          <mstrlayout:wRender name="searchBox">
          </mstrlayout:wRender>
        </td>
        <td class="{@cssPrefix}{@cssClass}MatchCaseBox">
          <mstrlayout:wRender name="matchCaseBox">
          </mstrlayout:wRender>
        </td>
      </tr>
    </table>
  </div>
</mstrlayout:layout>