<mstrlayout:layout xmlns:mstrlayout="http://microstrategy.com/web/2008/CSLAYOUT"
    on_model_set_readyState="mstr.behaviors.FetchPreview.on_model_set_readyState(view, model, rootTag)"
    on_set_enabled="mstr.behaviors.FetchPreview.on_set_enabled(view, model, rootTag)"
    on_set_visible="mstr.behaviors.FetchPreview.on_set_visible(view, model, rootTag)">
  <table title="{@tooltip}"
         cellspacing="0"
         style="{@cssText}"
         cellpadding="0"
         class="{@cssPrefix}{@cssClass}"
         id="{@id}">
    <tbody>
      <tr>
        <td class="{@cssPrefix}{@cssClass}CellText">
          <div class="{@cssPrefix}{@cssClass}Text">
          </div>
        </td>
        <td>
          <div class="{@cssPrefix}ToolButtonRounded">
            <img title="{@descriptors/fetchFirstCaption}"
                 onmouseout="mstr.behaviors.ToolButtonRounded.unhover(this.parentNode)"
                 style="background-image: url(../style/mstr/images/tbFetchFirstColumns.gif)"
                 src="../images/1ptrans.gif"
                 onmouseover="mstr.behaviors.ToolButtonRounded.hover(this.parentNode)"
                 align="absmiddle"
                 onclick="mstr.behaviors.FetchControl.fireFetchCommand('{@id}', 'FetchFirst')">
            </img>
          </div>
        </td>
        <td>
          <div class="{@cssPrefix}ToolButtonRounded">
            <img title="{@descriptors/fetchPreviousCaption}"
                 onmouseout="mstr.behaviors.ToolButtonRounded.unhover(this.parentNode)"
                 style="background-image: url(../style/mstr/images/tbFetchPreviousColumns.gif)"
                 src="../images/1ptrans.gif"
                 onmouseover="mstr.behaviors.ToolButtonRounded.hover(this.parentNode)"
                 align="absmiddle"
                 onclick="mstr.behaviors.FetchControl.fireFetchCommand('{@id}', 'FetchPrevious')">
            </img>
          </div>
        </td>
        <td class="{@cssPrefix}{@cssClass}CellPages">
          <div>
          </div>
        </td>
        <td>
          <div class="{@cssPrefix}ToolButtonRounded">
            <img title="{@descriptors/fetchNextCaption}"
                 onmouseout="mstr.behaviors.ToolButtonRounded.unhover(this.parentNode)"
                 style="background-image: url(../style/mstr/images/tbFetchNextColumns.gif)"
                 src="../images/1ptrans.gif"
                 onmouseover="mstr.behaviors.ToolButtonRounded.hover(this.parentNode)"
                 align="absmiddle"
                 onclick="mstr.behaviors.FetchControl.fireFetchCommand('{@id}', 'FetchNext')">
            </img>
          </div>
        </td>
        <td>
          <div class="{@cssPrefix}ToolButtonRounded">
            <img title="{@descriptors/fetchLastCaption}"
                 onmouseout="mstr.behaviors.ToolButtonRounded.unhover(this.parentNode)"
                 style="background-image: url(../style/mstr/images/tbFetchLastColumns.gif)"
                 src="../images/1ptrans.gif"
                 onmouseover="mstr.behaviors.ToolButtonRounded.hover(this.parentNode)"
                 align="absmiddle"
                 onclick="mstr.behaviors.FetchControl.fireFetchCommand('{@id}', 'FetchLast')">
            </img>
          </div>
        </td>
      </tr>
    </tbody>
  </table>
</mstrlayout:layout>
