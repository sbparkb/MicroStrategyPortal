		<definition>
			<property name="marginTabEnabled" type="Boolean" />
		</definition>
		<assignments>
			<property name="categoryText">{desc:mstrWeb.1897|Page Setup}</property>
			<property name="titleText">{desc:mstrWeb.3688|Margins}</property>
			<property name="fields">MarginLeft|MarginTop|MarginRight|MarginBottom|minimumPrintArea</property>
			
