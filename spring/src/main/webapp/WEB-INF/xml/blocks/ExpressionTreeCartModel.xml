<!DOCTYPE block SYSTEM "../dtds/blockLibrary.dtd">
	<!--
		ExpressionTreeCartModel is a data model which contains a TreeModel, "available",
		and an ExpressionModel, "selected".  ExpressionListCartModel typically is used
		to create conditions in "selected" using objects in "available".
	-->
	<block name="ExpressionTreeCartModel" inherits="ListCartModel">
		<assignments>
			<property name="scriptClass">mstr.models.ExpressionTreeCart</property>
		</assignments>
	</block>
