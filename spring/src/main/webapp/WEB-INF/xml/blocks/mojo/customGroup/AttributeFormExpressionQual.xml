<!DOCTYPE block SYSTEM "../dtds/blockLibrary.dtd">
<!--
This block contains properties for Attribute Form Expression Qualification.
-->

<block name="AttributeFormExpressionQual" inherits="ExpNode">
    <definition>
        <!-- Attribute ObjectInfo -->
        <property name="a" type="Block" />
        
        <!-- Attribute Form ObjectInfo -->
        <property name="fm" type="Block" />
        
        <!-- Function (operator) -->
        <property name="fn" type="Integer" />
        
        <!-- Function type if not present it is assumed to be generic. -->
        <property name="fnt" type="Integer" />
    
        <!-- Attribute2 ObjectInfo -->
        <property name="a2" type="Block" />
        
        <!-- Attribute Form2 ObjectInfo -->
        <property name="fm2" type="Block" />
        
        <!-- Attribute3 ObjectInfo (used in between comparison)-->
        <property name="a3" type="Block" />
        
        <!-- Attribute Form3 ObjectInfo (used in between comparison) -->
        <property name="fm3" type="Block" />
        
    
    </definition>
</block>
