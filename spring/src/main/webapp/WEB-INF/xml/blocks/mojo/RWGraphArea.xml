<!DOCTYPE block SYSTEM "../dtds/blockLibrary.dtd">
<!--
    This Block represents an area in a graph.
--> 
<block name="RWGraphArea" inherits="GraphArea,RWObject">
    <definition>
        <!-- The target control group bys  -->
        <property name="tgbs" type="String"/>
        <!--  control keys of the area  -->
        <property name="cks" type="String"/>                
        <!--  indicates the target type 0 - default,  1 - dataset   -->
        <property name="tty" type="Integer"/>                
        </definition>
</block>
