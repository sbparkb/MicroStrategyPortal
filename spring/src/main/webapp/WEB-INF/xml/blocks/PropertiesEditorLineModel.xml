		<definition>
			<property name="lineTabEnabled" type="Boolean" />
			<property name="LineOrientation" type="Block" />
		</definition>
		<assignments>
			<property name="categoryText">{desc:mstrWeb.434|Properties}</property>
			<property name="titleText">{desc:mstrWeb.2059|Line}</property>
			<property name="fields">LineOrientation</property>
			<property name="visibleField">lineTabEnabled</property>
			<property name="lineTabEnabled">true</property>
			<property name="LineOrientation">
				<block name="HOrientationModel" />
			</property>
		</assignments>
	</block>
