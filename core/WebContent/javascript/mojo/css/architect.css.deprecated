    margin-left: 2px;
    border-bottom: 1px dotted #AAA;
    cursor: pointer;
    position: relative;
    height: 14px;
}

.mstrmojo-ar-UnitList .item:hover {
    background-color: #EEE;
    color: #000;
}

.mstrmojo-ar-UnitList .item.selected {
    font-weight: bold;
}

.mstrmojo-ar-UnitList div.item.tp151{ background-position: left -580px; } /*logical view*/
.mstrmojo-ar-UnitList div.item.tp14{ background-position: left -528px; } /*user hierarchy*/
.mstrmojo-ar-UnitList div.item.tp43{ background-position: left -632px; } /*transformations*/
.mstrmojo-ar-UnitList div.item.tp70{ background-position: left -475px; } /*layer*/

