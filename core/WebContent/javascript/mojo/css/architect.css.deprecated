/** Start Tree DropZones code. **/
.mstrmojo-SimpleTree.mask,
.mstrmojo-SimpleTree.mask:hover {
    overflow: hidden !important;
}

.mstrmojo-SimpleTree.mask .item-n {
    -webkit-transition: all 100ms;
    transition: all 100ms;
    -moz-transition: all 100ms;
}

.mstrmojo-SimpleTree li.dzMask > .item {
