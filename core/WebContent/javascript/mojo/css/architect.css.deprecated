    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-right: 24px;
}

.mstrmojo-ar-ulist-icn-ren {
    background: url(images/wh/panel_icons.png) no-repeat 2px -623px;
    position: absolute;
    padding: 8px;
    opacity: 0.2;
    right: 7px;
    top: 7px;
    transition: opacity 200ms;
    border: 1px solid transparent;
    margin-right: 20px;
}

.mstrmojo-ar-ulist-icn-del {
    background: url(images/wh/panel_icons.png) no-repeat 2px -122px;
    position: absolute;
    padding: 8px;
    opacity: 0.2;
    right: 7px;
    top: 7px;
    transition: opacity 200ms;
    border: 1px solid transparent;
}

.mstrmojo-ar-UnitList .item:hover .mstrmojo-ar-ulist-icn-ren,
.mstrmojo-ar-UnitList .item:hover .mstrmojo-ar-ulist-icn-del {
    opacity: 0.6;
}

.mstrmojo-ar-UnitList .item:hover .mstrmojo-ar-ulist-icn-ren:hover {
    opacity: 1;
    border: 1px solid #CCC;
    background: url(images/wh/panel_icons.png) no-repeat 2px -623px, -webkit-linear-gradient(top, #FFF 0%,#E8E8E8 100%);
    background: url(images/wh/panel_icons.png) no-repeat 2px -623px, -moz-linear-gradient(top, #FFF 0%,#E8E8E8 100%);
    background: url(images/wh/panel_icons.png) no-repeat 2px -623px, linear-gradient(top, #FFF 0%,#E8E8E8 100%);
    border-radius: 2px;
}

.mstrmojo-ar-UnitList .item:hover .mstrmojo-ar-ulist-icn-del:hover {
    opacity: 1;
    border: 1px solid #CCC;
    background: url(images/wh/panel_icons.png) no-repeat 2px -122px, -webkit-linear-gradient(top, #FFF 0%,#E8E8E8 100%);
    background: url(images/wh/panel_icons.png) no-repeat 2px -122px, -moz-linear-gradient(top, #FFF 0%,#E8E8E8 100%);
    background: url(images/wh/panel_icons.png) no-repeat 2px -122px, linear-gradient(top, #FFF 0%,#E8E8E8 100%);
