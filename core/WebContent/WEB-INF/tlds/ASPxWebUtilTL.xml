    <tag>
	<name>exportToExcel</name> <!-- ASP.NET Control Name (ASP.Net Exclusive)-->
        <tagclass/>
        <bodycontent>ASP</bodycontent>
        <description>This tag is used when the excelDirectExport flag is set to true.
                     Reports are exported using temp files to support Unicode characters.
		</description>
        <usage>
            &lt;web:ifFeature runat="server" name="excelDirectExport" type="config" value="true"&gt;
		    	&lt;web:mthen runat="server"&gt;
                    &lt;web:exporttoExcel runat="server"  beanName="eb" /&gt;
                &lt;/web:mthen&gt;
                &lt;web:melse runat="server"&gt;
                    &lt;web:displayBean runat="server" bean="eb" /&gt;
                &lt;/web:melse&gt;
            &lt;/web:ifFeature&gt;
        </usage>
        <attribute>
            <name>beanName</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <description>The name of the bean to be loaded. (ExportBean - eb)</description>
        </attribute>
        <attribute>
            <name>runat</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <description>Indicates that the ASP.Net control should be processed on the server</description>
        </attribute>
        <attribute>
            <name>styleName</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>getTarget</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>ignoreCurrent</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>status</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>renderGroupList</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
