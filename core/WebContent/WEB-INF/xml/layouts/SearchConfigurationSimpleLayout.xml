    <colgroup>
      <col width="*"/>
      <col width="20"/>
      <col width="*"/>
    </colgroup>
    <tr>
      <td style="vertical-align: top">
        <div class="mstrSearchSections">
          <div id="nameSection">
            <mstrlayout:render name="searchNameTitle"/>
            <br />
            <mstrlayout:render name="searchNameTextInputBox"/>
            <br />
