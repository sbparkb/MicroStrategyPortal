<!DOCTYPE mstrlayout:layout SYSTEM "mstrlayout.dtd">
<mstrlayout:layout>
	<mstrlayout:bContext defaultContentType="JSON" />
			<mstrlayout:block name="BlockContainer">
				<mstrlayout:bSetProperty path="views">
					<mstrlayout:block name="FontFormatEditor">
					  <!-- Help Button -->
						<mstrlayout:bSetProperty path="helpPath" method="getHelpPath"/>
						<mstrlayout:bSetProperty path="helpTopic" method="getHelpTopic"/>					
					  <!-- Model Path -->
						<mstrlayout:bSetProperty path="modelPath" method="getRequestParameterValue">
							<mstrlayout:argument type="String" value="modelPath" />
							<mstrlayout:argument type="String" value="opener/model" />
						</mstrlayout:bSetProperty>
					  <!-- Font Preview Fields -->
						<mstrlayout:bSetProperty path="contentView/children[0]/nameModelPath" method="getRequestParameterValue">
							<mstrlayout:argument type="String" value="contentView/children/0/nameModelPath" />
							<mstrlayout:argument type="String" value="" />
						</mstrlayout:bSetProperty>
						<mstrlayout:bSetProperty path="contentView/children[0]/styleModelPath" method="getRequestParameterValue">
							<mstrlayout:argument type="String" value="contentView/children/0/styleModelPath" />
							<mstrlayout:argument type="String" value="" />
						</mstrlayout:bSetProperty>
						<mstrlayout:bSetProperty path="contentView/children[0]/sizeModelPath" method="getRequestParameterValue">
							<mstrlayout:argument type="String" value="contentView/children/0/sizeModelPath" />
							<mstrlayout:argument type="String" value="" />
						</mstrlayout:bSetProperty>
						<mstrlayout:bSetProperty path="contentView/children[0]/underlineField" method="getRequestParameterValue">
							<mstrlayout:argument type="String" value="contentView/children/0/underlineField" />
							<mstrlayout:argument type="String" value="" />
						</mstrlayout:bSetProperty>
						<mstrlayout:bSetProperty path="contentView/children[0]/strikeoutField" method="getRequestParameterValue">
							<mstrlayout:argument type="String" value="contentView/children/0/strikeoutField" />
							<mstrlayout:argument type="String" value="" />
						</mstrlayout:bSetProperty>
						<mstrlayout:bSetProperty path="contentView/children[0]/colorModelPath" method="getRequestParameterValue">
							<mstrlayout:argument type="String" value="contentView/children/0/colorModelPath" />
							<mstrlayout:argument type="String" value="" />
				</mstrlayout:bSetProperty>
			</mstrlayout:block>
		</mstrlayout:bSetProperty>
	</mstrlayout:block>
</mstrlayout:layout>
