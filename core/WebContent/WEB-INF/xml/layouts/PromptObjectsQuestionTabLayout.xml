<!DOCTYPE mstrlayout:layout SYSTEM "mstrlayout.dtd"><!-- in pageconfig.xml this tab's name is tabPromptQId, so its tab id is tabPromptQId --><mstrlayout:layout>	<span class="mstrTitle">		<mstrlayout:render name="Descriptor">			<mstrlayout:argument type="int" value="6099"/>        </mstrlayout:render>	</span>		<!-- DESCRIPTOR: List of objects to display-->	<p/>	<table style="position:relative;left:20px;"><!--		<tr>			<td colspan="2">				<mstrlayout:if name="useTypeRestriction">					<mstrlayout:then>						<input type="radio" name="optype" id="op_type" value="all" checked="checked"/>					</mstrlayout:then>					<mstrlayout:else>						<input type="radio" name="optype" id="op_type" value="all" />					</mstrlayout:else>								</mstrlayout:if>				<label for="op_type"> *List all objects of this type*: </label>		  				<select name="opTypes" id="opTypes" class="promptDropDown">					<option id="12" value="12">Metrics</option>					<option id="4" value="4">Attributes</option>					<option id="1" value="1">Templates</option>					<option id="2" value="2">Custom Groups</option>					<option id="3" value="3">Consolidations</option>							</select>			</td>		</tr>-->		<tr>			<td colspan="2">				<mstrlayout:if name="usePredefinedList">					<mstrlayout:then>						<input type="radio" name="optype" id="opPredefined" value="redefined" cmdid="radioSelection" onclick="microstrategy.bone('tabPromptQId').updateRadioButton(event);" checked="checked"/>					</mstrlayout:then>					<mstrlayout:else>						<input type="radio" name="optype" id="opPredefined" value="redefined" cmdid="radioSelection" onclick="microstrategy.bone('tabPromptQId').updateRadioButton(event);"/>					</mstrlayout:else>								</mstrlayout:if>				<label for="opPredefined">					<mstrlayout:render name="Descriptor">						<mstrlayout:argument type="int" value="6100"/>        			</mstrlayout:render> 				</label>		<!-- DESCRIPTOR: Use a predefined list of objects:-->			</td>		</tr>				<tr>			<td>				<table style="margin-left:20px;">					<tr>						<td>							<select multiple="multiple" id="op_elem_pre_list" name="op_elem_pre_list" cmdid="setPreDefObjs" size="10" DPN="opPredefined" NONZERO="1" FLD="Use_a_predefined_list_of_objects" class="mstrElementList" onchange="microstrategy.bone('tabPromptQId').onClickOPPredefinedList(this)">								<mstrlayout:list id="objects" name="getPredefinedList">									<option>										<mstrlayout:attr name="addOptionAttr">											<mstrlayout:argument list="objects" type="com.microstrategy.web.app.transforms.AbstractPromptEditorTransform$ListItem"/>										</mstrlayout:attr>										<mstrlayout:render name="optionText">											<mstrlayout:argument list="objects" type="com.microstrategy.web.app.transforms.AbstractPromptEditorTransform$ListItem"/>										</mstrlayout:render>									</option>								</mstrlayout:list>												</select>						</td>						<td style="vertical-align:middle;">							<div>								<input type="button" id="op_up_elem" name="op_up_elem" class="mstrIcon-btn mstrIcon-btnArrowUp" onclick="microstrategy.bone('tabPromptQId').moveUpOPItem('op_elem_pre_list')"/><br/>								<input type="button" id="op_down_elem" name="op_down_elem" class="mstrIcon-btn mstrIcon-btnArrowDown" onclick="microstrategy.bone('tabPromptQId').moveDownOPItem('op_elem_pre_list')"/>							</div>						</td>					</tr>				</table>			</td>			<td>				<div>					<input type="button" id="op_add_elem" name="op_add_elem" cmdid="openObjSelector" class="mstrButton mstrPromptButtonBlock" style="top:10px;" onclick="microstrategy.bone('tabPromptQId').openMultiSelector('op_elem_pre_list', 'tabPromptQId', 'SET_OBJECTS_AVAILABLE_LIST',4, false);">
						<mstrlayout:attr attribute="value" method="getDescriptor"><mstrlayout:argument type="int" value="922" /></mstrlayout:attr>		<!-- DESCRIPTOR: Add...-->
					</input>					<input type="button" id="op_remove_elem" name="op_remove_elem" class="mstrButton mstrPromptButtonBlock" onclick="microstrategy.bone('tabPromptQId').removeItems('op_elem_pre_list');microstrategy.bone('tabPromptQId').refreshOPButtonStates();" style="top:30px;">						<mstrlayout:attr attribute="value" method="getDescriptor"><mstrlayout:argument type="int" value="190" /></mstrlayout:attr>		<!-- DESCRIPTOR: Remove-->					</input>					<input type="button" id="op_clear_elem" name="op_clear_elem" class="mstrButton mstrPromptButtonBlock" onclick="microstrategy.bone('tabPromptQId').removeAllItems('op_elem_pre_list');microstrategy.bone('tabPromptQId').refreshOPButtonStates();" style="top:50px;">
						<mstrlayout:attr attribute="value" method="getDescriptor"><mstrlayout:argument type="int" value="2827" /></mstrlayout:attr>		<!-- DESCRIPTOR: Clear-->
					</input>				</div>			</td>		</tr>		<tr>			<td colspan="2">				<mstrlayout:if name="useSearchResults">					<mstrlayout:then>						<input type="radio" name="optype" id="opSearch" value="search" cmdid="radioSelection" onclick="microstrategy.bone('tabPromptQId').updateRadioButton(event);microstrategy.bone('tabPromptQId').updateText(getObj('op_search_value'));" checked="checked"/>					</mstrlayout:then>					<mstrlayout:else>						<input type="radio" name="optype" id="opSearch" value="search" cmdid="radioSelection" onclick="microstrategy.bone('tabPromptQId').updateRadioButton(event);microstrategy.bone('tabPromptQId').updateText(getObj('op_search_value'));"/>					</mstrlayout:else>								</mstrlayout:if>				<label for="opSearch"> 					<mstrlayout:render name="Descriptor"><mstrlayout:argument type="int" value="5245"/></mstrlayout:render>        			<br/>         		</label>		<!-- DESCRIPTOR: Use the result of a search object:-->			</td>		</tr>		<tr>			<td>				<input type="text" name="op_search_value" id="op_search_value" readonly="readonly" cmdid="setSearchObj" DPN="opSearch" NONZERO="1" FLDID="5245" class="mstrInputTextLong" style="left:20px">					<mstrlayout:attr name="addSearchNameAttrs"/>				</input>			</td>			<td>				<input type="button" name="op_search_selector" id="op_search_selector" cmdid="openSearchSelector" class="mstrButton mstrPromptButtonBlock" onclick="microstrategy.bone('tabPromptQId').openMultiSelector('op_search', 'tabPromptQId', 'SET_OBJECTS_SEARCH', 11, true);"> <!-- @todo, should not use onmenusubmitform() !!!-->
					<mstrlayout:attr attribute="value" method="getDescriptor"><mstrlayout:argument type="int" value="5231" /></mstrlayout:attr>		<!-- DESCRIPTOR: Select Search... -->
				</input>			</td>		</tr>				</table>		</mstrlayout:layout>