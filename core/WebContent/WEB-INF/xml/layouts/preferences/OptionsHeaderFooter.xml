<!DOCTYPE mstrlayout:layout SYSTEM "mstrlayout.dtd">
<mstrlayout:layout>
  <div class="mstrPanelPortrait">
    <div class="mstrPanelTitleBar">
        <span class="mstrPanelTitle">
            <mstrlayout:render name="title"/>
        </span>
    </div>
    <div class="mstrPanelBody">
      <table cellspacing="0" cellpadding="0" class="preferenceList mstrPreferencesListPrintHeaderFooter">
        <tr>
          <td valign="top" colspan="3">
            <mstrlayout:render name="MacroPulldown"/>
            <br /><br />
          </td>
        </tr>
        
		<tr>
          <td align="left" valign="top" colspan="3">
            <span class="prefs-content-bold-text">
              <mstrlayout:render name="descriptor">  <!-- Header: -->
                  <mstrlayout:argument type="String" value="mstrWeb.1932"/>
              </mstrlayout:render>
            </span>
          </td>
        </tr>
        <tr>
          <td align="center" valign="top" style="width:33%">
            <mstrlayout:render name="HeaderLeft"/>
          </td>
          <td align="center" valign="top" style="width:33%">
            <mstrlayout:render name="HeaderCenter"/>
          </td>
          <td align="center" valign="top" style="width:33%">
            <mstrlayout:render name="HeaderRight"/>
          </td>
        </tr>

        <tr>
          <td align="left" valign="top" colspan="3">
            <br /><br /><br />
            <span class="prefs-content-bold-text">
              <mstrlayout:render name="descriptor">  <!-- Footer: -->
                  <mstrlayout:argument type="String" value="mstrWeb.1933"/>
              </mstrlayout:render>
            </span>
          </td>
        </tr>
        <tr>
          <td align="center" valign="top" style="width:33%">
            <mstrlayout:render name="FooterLeft"/>
          </td>
          <td align="center" valign="top" style="width:33%">
            <mstrlayout:render name="FooterCenter"/>
          </td>
          <td align="center" valign="top" style="width:33%">
            <mstrlayout:render name="FooterRight"/>
          </td>
        </tr>

        <tr>
          <td align="left" valign="top" colspan="3">
            <br /><br /><br />
          </td>
        </tr>

      </table>
    </div>

    <div class="mstrPanelButtonBar">
      <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <colgroup>
          <col width="70%"/>
          <col width="30%"/>
        </colgroup>
        <tr>
          <td valign="top">
            <mstrlayout:render name="ApplyChanges"/>
            <mstrlayout:render name="CancelButton"/>
          </td>
        </tr>
      </table>
    </div>
  </div>
</mstrlayout:layout>
