<!DOCTYPE mstrlayout:layout SYSTEM "mstrlayout.dtd">
<mstrlayout:layout>
  <mstrlayout:include file="/WEB-INF/xml/layouts/PromptsClassicBaseLayout.xml">
      <mstrlayout:replace slot="warningHeaderMessage">
        <div class="promptWarningHeaderMessage">
            <mstrlayout:render name="warningHeaderMessage"/>
          </div>
    	</mstrlayout:replace>
      <mstrlayout:replace slot="summaryButtons">
          <mstrlayout:render name="executeButton"/>
          <mstrlayout:render name="saveButton"/>
          <mstrlayout:render name="cancelButton"/>
    	</mstrlayout:replace>
      <mstrlayout:replace slot="buttons">
          <mstrlayout:render name="executeButton"/>
          <mstrlayout:render name="saveButton"/>
          <mstrlayout:render name="cancelButton"/>
      </mstrlayout:replace>
  </mstrlayout:include>
</mstrlayout:layout>
