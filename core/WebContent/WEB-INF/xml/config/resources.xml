<resource-feed-config>
  <parameters>
    <parameter name="ProxyURL" value=""/>
    <parameter name="ProxyPort" value=""/>
    <parameter name="Default-User-Agent" value="Mozilla/4.0 (compatible; MSIE 6.0; Windows NT 5.0; .NET CLR 1.1.4322; .NET CLR 2.0.50727)"/>
    <parameter name="set-status-code-from-response" value="true"/>
  </parameters>
  <!-- 
  	The domainsyntax accepted is:

  	<domain>www.a.com</domain> 
  	<domain>www.b.com</domain> 

   -->
  <allow-domains></allow-domains>
  <!-- 
  	The content type specified must be a part of the content type sent by the server. 
  	For instance "text/xml" would be valid if the content type returned by the server is "text/xml; charset=UTF-8" 
  -->
  <allow-content-types>
    <content-type>*</content-type>
  </allow-content-types>
</resource-feed-config>