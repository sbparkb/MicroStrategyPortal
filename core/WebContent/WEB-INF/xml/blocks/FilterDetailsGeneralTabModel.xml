<!DOCTYPE block SYSTEM "../dtds/blockLibrary.dtd">
<block inherits="ChangeTrackingModel" name="FilterDetailsGeneralTabModel">
  <definition>
    <!-- Report Details -->
    <property name="RDRD" type="Block"/>
    <!-- includeReportDescription -->
    <property name="RDPD" type="Block"/>
    <!-- includePromptDetails -->
    <property name="RDFD" type="Block"/>
    <!-- includeFilterDetails -->
    <property name="RDTD" type="Block"/>
    <!-- includeTemplateDetails -->
    <!-- Prompt Details -->
    <property name="PDTI" type="Block"/>
    <!-- includePromptTitles -->
    <property name="PDUA" type="Block"/>
    <!-- replacementPromptString -->
    <property name="sanep" type="Block"/>
    <!-- showAttribNameForAEPrompts -->
    <property name="IUPD" type="Block"/>
    <!-- Include Unused Prompts in Display -->
    <!-- Miscellaneous -->
    <property name="daon" type="Block"/>
    <!-- mdDelimeter -->
    <property name="FDOA" type="Block"/>
    <!-- useAliasesInFilterDetails -->
  </definition>
  <assignments>
    <property name="enabled" type="Boolean">true</property>
    <property name="RDRD">
      <block name="TriStateListModel"/>
    </property>
    <!-- Report Details -->
    <property name="RDPD">
      <block name="TriStateListModel"/>
    </property>
    <property name="RDFD">
      <block name="TriStateListModel"/>
    </property>
    <property name="RDTD">
      <block name="TriStateListModel"/>
    </property>
    <!-- Prompt Details -->
    <property name="PDTI">
      <block name="ListModel">
        <property name="valueForm">dssid</property>
        <property name="items">
          <list>
            <block listElementKey="K517" name="ItemInfo">
              <property name="dssid">-1</property>
              <property name="n">{desc:mstrWeb.5404|Inherit}</property>
            </block>
            <block listElementKey="K518" name="ItemInfo">
              <property name="dssid">0</property>
              <property name="n">{desc:mstrWeb.5405|No Title or Index}</property>
            </block>
            <block listElementKey="K519" name="ItemInfo">
              <property name="dssid">1</property>
              <property name="n">{desc:mstrWeb.5406|Title and Index}</property>
            </block>
          </list>
        </property>
      </block>
    </property>
    <property name="PDUA">
      <block name="ListModel">
        <property name="valueForm">dssid</property>
        <property name="items">
          <list>
            <block listElementKey="K520" name="ItemInfo">
              <property name="dssid">-1</property>
              <property name="n">{desc:mstrWeb.5404|Inherit}</property>
            </block>
            <block listElementKey="K521" name="ItemInfo">
              <property name="dssid">0</property>
              <property name="n">{desc:mstrWeb.2797|Blank}</property>
            </block>
            <block listElementKey="K522" name="ItemInfo">
              <property name="dssid">1</property>
              <property name="n">{desc:mstrWeb.1068|Prompt not answered}</property>
            </block>
            <block listElementKey="K523" name="ItemInfo">
              <property name="dssid">2</property>
              <property name="n">{desc:mstrWeb.3520|No Selection}</property>
            </block>
            <block listElementKey="K524" name="ItemInfo">
              <property name="dssid">3</property>
              <property name="n">{desc:mstrWeb.5667|All/None}</property>
            </block>
          </list>
        </property>
      </block>
    </property>
    <property name="sanep">
      <block name="ListModel">
        <property name="valueForm">dssid</property>
        <property name="items">
          <list>
            <block listElementKey="K525" name="ItemInfo">
              <property name="dssid">-1</property>
              <property name="n">{desc:mstrWeb.5404|Inherit}</property>
            </block>
            <block listElementKey="K526" name="ItemInfo">
              <property name="dssid">0</property>
              <property name="n">{desc:mstrWeb.63|Never}</property>
            </block>
            <block listElementKey="K527" name="ItemInfo">
              <property name="dssid">1</property>
              <property name="n">{desc:mstrWeb.5407|Once}</property>
            </block>
            <block listElementKey="K528" name="ItemInfo">
              <property name="dssid">2</property>
              <property name="n">{desc:mstrWeb.5408|Repeated before each element}</property>
            </block>
          </list>
        </property>
      </block>
    </property>
    <property name="IUPD">
      <block name="TriStateListModel"/>
    </property>
    <!-- Miscellaneous -->
    <property name="daon">
      <block name="ListModel">
        <property name="valueForm">dssid</property>
        <property name="items">
          <list>
            <block listElementKey="K529" name="ItemInfo">
              <property name="dssid">-1</property>
              <property name="n">{desc:mstrWeb.5404|Inherit}</property>
            </block>
            <block listElementKey="K530" name="ItemInfo">
              <property name="dssid">0</property>
              <property name="n">{desc:mstrWeb.63|Never}</property>
            </block>
            <block listElementKey="K531" name="ItemInfo">
              <property name="dssid">1</property>
              <property name="n">{desc:mstrWeb.62|Always}</property>
            </block>
            <block listElementKey="K532" name="ItemInfo">
              <property name="dssid">2</property>
              <property name="n">{desc:mstrWeb.5409|Only when the name contains special characters}</property>
            </block>
          </list>
        </property>
      </block>
    </property>
    <property name="FDOA">
      <block name="TriStateListModel"/>
    </property>
  </assignments>
</block>
