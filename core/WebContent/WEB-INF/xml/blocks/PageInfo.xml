        </property>
      </block>
    </property>
    <property name="userInfo">
      <block name="BaseModel">
        <property name="decimalSeparator" type="String">.</property>
        <property name="dpi" type="Integer">96</property>
      </block>
    </property>
  </assignments>
</block>
