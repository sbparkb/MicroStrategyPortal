<!DOCTYPE block SYSTEM "../dtds/blockLibrary.dtd">
	<block name="PropertiesEditorModel" inherits="BoneTranslatorManager">
		<assignments>
			<property name="scriptClass">mstr.bones.editors.PropertiesBoneTranslatorManager</property>
			<property name="boneId">rwb_viewer</property>
			<property name="boneCmdId">PropertyChanges</property>
			<property name="selectedIndicesList">0</property>
			<property name="valueForm">id</property>
			<property name="lockedTab" type="String">-1</property>
			<property name="disableList" type="Boolean">false</property>

			<property name="showGridSelectors" type="Boolean">false</property>
			<property name="showPanelSelector" type="Boolean">false</property>
			<property name="showCancelButton" type="Boolean">true</property>
			
			<property name="CommandEnabledOpenWebHyperLinkEditor" type="Boolean">true</property>

			<property name="edtUnitSelector" type="Block">
				<block name="ListModel">
					<property name="valueForm">dssid</property>
					<property name="visible" type="Boolean">false</property>
					<property name="enabled" type="Boolean">false</property>
				</block>
			</property>
			<property name="edtValueSelector" type="Block">
				<block name="ListModel">
					<property name="valueForm">dssid</property>
					<property name="visible" type="Boolean">false</property>
					<property name="enabled" type="Boolean">false</property>
                    <property name="formatEnabled" type="Boolean">false</property>
				</block>
			</property>

			<property name="items">
				<list>
					<block name="PropertiesEditorGeneralModel" listElementKey="general" />
					<block name="PropertiesEditorLayoutModel" listElementKey="layout" />
					<block name="PropertiesEditorImageModel" listElementKey="image" />
					<block name="PropertiesEditorLineModel" listElementKey="line" />
					<block name="PropertiesEditorGridModel" listElementKey="grid" />
					<block name="PropertiesEditorWidgetModel" listElementKey="widget" />
					<block name="PropertiesEditorSelectorModel" listElementKey="selector" />
					<block name="PropertiesEditorAdvancedModel" listElementKey="advanced" />
					<block name="PropertiesEditorButtonModel" listElementKey="button" />
					<block name="PropertiesEditorColorModel" listElementKey="color" />
					<block name="PropertiesEditorFontModel" listElementKey="font" />
					<block name="PropertiesEditorNumberModel" listElementKey="number" />
					<block name="PropertiesEditorAlignmentModel" listElementKey="align" />
					<block name="PropertiesEditorEffectsModel" listElementKey="effects" />
					<block name="PropertiesEditorThemeModel" listElementKey="theme" />
				</list>
			</property>
		</assignments>
	</block>
